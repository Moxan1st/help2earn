# Help2Earn Backend Environment Variables

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/help2earn

# Blockchain (Sepolia Testnet)
SEPOLIA_RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_ALCHEMY_KEY
MINTER_PRIVATE_KEY=your_private_key_here
TOKEN_CONTRACT_ADDRESS=0x...
DISTRIBUTOR_CONTRACT_ADDRESS=0x...

# Google AI (Gemini)
GOOGLE_API_KEY=your_gemini_api_key

# Image Storage - Google Cloud Storage
GCS_PROJECT_ID=your_gcp_project_id
GCS_BUCKET_NAME=help2earn-images
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Server
HOST=0.0.0.0
PORT=8000
DEBUG=false
